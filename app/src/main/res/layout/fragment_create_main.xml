<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.blq.qrcode.fragment.CreateMainFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <include
                android:id="@+id/fl_ll_ll_sms"
                layout="@layout/create_item"/>
            <include
                android:id="@+id/fl_ll_ll_contacts"
                layout="@layout/create_item"/>
            <include
                android:id="@+id/fl_ll_ll_http"
                layout="@layout/create_item"/>
            <include
                android:id="@+id/fl_ll_ll_text"
                layout="@layout/create_item"/>
        </LinearLayout>
    </ScrollView>
</FrameLayout>
